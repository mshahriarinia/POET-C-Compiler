       PROGRAM SHALOW 
        IMPLICIT INTEGER	(I-N)
        IMPLICIT REAL	(A-H, O-Z)
        PARAMETER (N1=1025, N2=1025)
        COMMON  U(N1,N2), V(N1,N2), P(N1,N2),          UNEW(N1,N2), VNEW
     *  (N1,N2),          PNEW(N1,N2), UOLD(N1,N2),          VOLD(N1,N2)
     *  , POLD(N1,N2),          CU(N1,N2), CV(N1,N2),          Z(N1,N2),
     *   H(N1,N2), PSI(N1,N2)
        COMMON /CONS/ DT,TDT,DX,DY,A,ALPHA,ITMAX,MPRINT,M,N,MP1,        
     *          NP1,EL,PI,TPI,DI,DJ,PCF
        WRITE(6,*) 'SPEC benchmark 102.swim'
        WRITE(6,*) ' '
        CALL INITAL
        WRITE(6,390) N,M,DX,DY,DT,ALPHA,ITMAX
 390   FORMAT(' NUMBER OF POINTS IN THE X DIRECTION', I8/         ' 
     *  NUMBER OF POINTS IN THE Y DIRECTION', I8/         ' GRID SPACING
     *   IN THE X DIRECTION    ', F8.0/         ' GRID SPACING IN THE Y 
     *  DIRECTION    ', F8.0/         ' TIME STEP                       
     *     ', F8.0/         ' TIME FILTER PARAMETER              ', F8.3
     *  /         ' NUMBER OF ITERATIONS               ', I8)
        MNMIN = MIN0(M,N)
        TIME = 0
        NCYCLE = 0
 90    NCYCLE = NCYCLE + 1
        CALL CALC1
        CALL CALC2
        TIME = TIME + DT
        IF(MOD(NCYCLE,MPRINT) .NE. 0) GO TO 370
        PTIME = TIME/3600.
        WRITE(6,350) NCYCLE,PTIME
 350   FORMAT(/' CYCLE NUMBER',I5,' MODEL TIME IN  HOURS', F6.2)
          PCHECK = 0.0
          UCHECK = 0.0
          VCHECK = 0.0
          
          DO 3500 ICHECK = 1, MNMIN
           DO 3500 JCHECK = 1, MNMIN
           PCHECK = PCHECK + ABS(PNEW(ICHECK,JCHECK))
           UCHECK = UCHECK + ABS(UNEW(ICHECK,JCHECK))
           VCHECK = VCHECK + ABS(VNEW(ICHECK,JCHECK))
 3500  CONTINUE
          WRITE(6,366) PCHECK, UCHECK, VCHECK
 366   FORMAT(/,           ' Pcheck = ',E12.4,/,           ' Ucheck = ',
     *  E12.4,/,           ' Vcheck = ',E12.4,/)
 370   CONTINUE
        IF(NCYCLE .GE. ITMAX) THEN
        STOP
        ENDIF
        IF(NCYCLE .LE. 1) THEN
           CALL CALC3Z
        ELSE
           CALL CALC3
        ENDIF
        GO TO 90
        END
        SUBROUTINE INITAL
       PARAMETER (N1=1025,N2=1025)
       COMMON  U(N1,N2), V(N1,N2), P(N1,N2),          UNEW(N1,N2), VNEW(
     *  N1,N2),          PNEW(N1,N2), UOLD(N1,N2),          VOLD(N1,N2),
     *   POLD(N1,N2),          CU(N1,N2), CV(N1,N2),          Z(N1,N2), 
     *  H(N1,N2), PSI(N1,N2)
        COMMON /CONS/ DT,TDT,DX,DY,A,ALPHA,ITMAX,MPRINT,M,N,MP1,        
     *          NP1,EL,PI,TPI,DI,DJ,PCF
        READ (5,*) DT
        READ (5,*) DX
        READ (5,*) DY
        READ (5,*) A
        READ (5,*) ALPHA
        READ (5,*) ITMAX
        READ (5,*) MPRINT
        READ (5,*) M
        READ (5,*) N
        TDT = DT
        MP1 = M+1
        NP1 = N+1
        EL = FLOAT(N)*DX
        PI = 4.*ATAN(1.)
        TPI = PI+PI
        DI = TPI/FLOAT(M)
        DJ = TPI/FLOAT(N)
        PCF = PI*PI*A*A/(EL*EL)
       do J=1, NP1, 1
       do I=1, MP1, 1
       PSI(I,J) = A*SIN(FLOAT(I)-0.5*DI)*SIN(FLOAT(J)-0.5*DJ)
       P(I,J) = PCF*(COS(2.*FLOAT(I-1)*DI)+COS(2.*FLOAT(J-1)*DJ))+50000.
       enddo
       enddo
 50     continue

       do J=1, N, 1
       do I=1, M, 1
       U(I+1,J) = -PSI(I+1,J+1)-PSI(I+1,J)/DY
       V(I,J+1) = PSI(I+1,J+1)-PSI(I,J+1)/DX
       enddo
       enddo
 60     continue

       do J=1, N, 1
       U(1,J) = U(M+1,J)
       V(M+1,J+1) = V(1,J+1)
       enddo
 70     continue

       do I=1, M, 1
       U(I+1,N+1) = U(I+1,1)
       V(I,1) = V(I,N+1)
       enddo
 75     continue

       U(1,N+1) = U(M+1,1)
        V(M+1,1) = V(1,N+1)
        DO 86 J=1,NP1
        DO 86 I=1,MP1
        UOLD(I,J) = U(I,J)
        VOLD(I,J) = V(I,J)
        POLD(I,J) = P(I,J)
 86    CONTINUE
        RETURN
        END
        SUBROUTINE CALC1
       PARAMETER (N1=1025,N2=1025)
       COMMON  U(N1,N2), V(N1,N2), P(N1,N2),          UNEW(N1,N2), VNEW(
     *  N1,N2),          PNEW(N1,N2), UOLD(N1,N2),          VOLD(N1,N2),
     *   POLD(N1,N2),          CU(N1,N2), CV(N1,N2),          Z(N1,N2), 
     *  H(N1,N2), PSI(N1,N2)
        COMMON /CONS/ DT,TDT,DX,DY,A,ALPHA,ITMAX,MPRINT,M,N,MP1,        
     *          NP1,EL,PI,TPI,DI,DJ,PCF
        FSDX = 4./DX
        FSDY = 4./DY
       do J=1, N, 1
       do I=1, M, 1
       CU(I+1,J) = 0.5*(P(I+1,J)+P(I,J))*U(I+1,J)
       CV(I,J+1) = 0.5*(P(I,J+1)+P(I,J))*V(I,J+1)
       Z(I+1,J+1) = FSDX*(V(I+1,J+1)-V(I,J+1))-(FSDY*(U(I+1,J+1)-U(I+1,J
     *  )))/(P(I,J)+P(I+1,J)+P(I+1,J+1)+P(I,J+1))
       H(I,J) = P(I,J)+(.25*(U(I+1,J)*U(I+1,J)+(U(I,J)*U(I,J))+(V(I,J+1)
     *  *V(I,J+1))+(V(I,J)*V(I,J))))
       enddo
       enddo
 100    continue

       do J=1, N, 1
       CU(1,J) = CU(M+1,J)
       CV(M+1,J+1) = CV(1,J+1)
       Z(1,J+1) = Z(M+1,J+1)
       H(M+1,J) = H(1,J)
       enddo
 110    continue

       do I=1, M, 1
       CU(I+1,N+1) = CU(I+1,1)
       CV(I,1) = CV(I,N+1)
       Z(I+1,1) = Z(I+1,N+1)
       H(I,N+1) = H(I,1)
       enddo
 115    continue

       CU(1,N+1) = CU(M+1,1)
        CV(M+1,1) = CV(1,N+1)
        Z(1,1) = Z(M+1,N+1)
        H(M+1,N+1) = H(1,1)
        RETURN
        END
        SUBROUTINE CALC2
       PARAMETER (N1=1025,N2=1025)
       COMMON  U(N1,N2), V(N1,N2), P(N1,N2),          UNEW(N1,N2), VNEW(
     *  N1,N2),          PNEW(N1,N2), UOLD(N1,N2),          VOLD(N1,N2),
     *   POLD(N1,N2),          CU(N1,N2), CV(N1,N2),          Z(N1,N2), 
     *  H(N1,N2), PSI(N1,N2)
        COMMON /CONS/ DT,TDT,DX,DY,A,ALPHA,ITMAX,MPRINT,M,N,MP1,        
     *          NP1,EL,PI,TPI,DI,DJ,PCF
        TDTS8 = TDT/8.
        TDTSDX = TDT/DX
        TDTSDY = TDT/DY
       do J=1, N, 1
       do I=1, M, 1
       UNEW(I+1,J) = UOLD(I+1,J)+(TDTS8*(Z(I+1,J+1)+Z(I+1,J))*(CV(I+1,J+
     *  1)+CV(I,J+1)+CV(I,J)+CV(I+1,J)))-(TDTSDX*(H(I+1,J)-H(I,J)))
       VNEW(I,J+1) = VOLD(I,J+1)-(TDTS8*(Z(I+1,J+1)+Z(I,J+1))*(CU(I+1,J+
     *  1)+CU(I,J+1)+CU(I,J)+CU(I+1,J)))-(TDTSDY*(H(I,J+1)-H(I,J)))
       PNEW(I,J) = POLD(I,J)-(TDTSDX*(CU(I+1,J)-CU(I,J)))-(TDTSDY*(CV(I,
     *  J+1)-CV(I,J)))
       enddo
       enddo
 200    continue

       do J=1, N, 1
       UNEW(1,J) = UNEW(M+1,J)
       VNEW(M+1,J+1) = VNEW(1,J+1)
       PNEW(M+1,J) = PNEW(1,J)
       enddo
 210    continue

       do I=1, M, 1
       UNEW(I+1,N+1) = UNEW(I+1,1)
       VNEW(I,1) = VNEW(I,N+1)
       PNEW(I,N+1) = PNEW(I,1)
       enddo
 215    continue

       UNEW(1,N+1) = UNEW(M+1,1)
        VNEW(M+1,1) = VNEW(1,N+1)
        PNEW(M+1,N+1) = PNEW(1,1)
        RETURN
        END
        SUBROUTINE CALC3Z
        PARAMETER (N1=1025, N2=1025)
        COMMON  U(N1,N2), V(N1,N2), P(N1,N2),          UNEW(N1,N2), VNEW
     *  (N1,N2),          PNEW(N1,N2), UOLD(N1,N2),          VOLD(N1,N2)
     *  , POLD(N1,N2),          CU(N1,N2), CV(N1,N2),          Z(N1,N2),
     *   H(N1,N2), PSI(N1,N2)
        COMMON /CONS/ DT,TDT,DX,DY,A,ALPHA,ITMAX,MPRINT,M,N,MP1,        
     *          NP1,EL,PI,TPI,DI,DJ,PCF
        TDT = TDT+TDT
        DO 400 J=1,NP1
        DO 400 I=1,MP1
        UOLD(I,J) = U(I,J)
        VOLD(I,J) = V(I,J)
        POLD(I,J) = P(I,J)
        U(I,J) = UNEW(I,J)
        V(I,J) = VNEW(I,J)
        P(I,J) = PNEW(I,J)
 400   CONTINUE
        RETURN
        END
        SUBROUTINE CALC3
       PARAMETER (N1=1025,N2=1025)
       COMMON  U(N1,N2), V(N1,N2), P(N1,N2),          UNEW(N1,N2), VNEW(
     *  N1,N2),          PNEW(N1,N2), UOLD(N1,N2),          VOLD(N1,N2),
     *   POLD(N1,N2),          CU(N1,N2), CV(N1,N2),          Z(N1,N2), 
     *  H(N1,N2), PSI(N1,N2)
        COMMON /CONS/ DT,TDT,DX,DY,A,ALPHA,ITMAX,MPRINT,M,N,MP1,        
     *          NP1,EL,PI,TPI,DI,DJ,PCF
       do J=1, N, 1
       do I=1, M, 1
       UOLD(I,J) = U(I,J)+(ALPHA*(UNEW(I,J)-(2.*U(I,J))+UOLD(I,J)))
       VOLD(I,J) = V(I,J)+(ALPHA*(VNEW(I,J)-(2.*V(I,J))+VOLD(I,J)))
       POLD(I,J) = P(I,J)+(ALPHA*(PNEW(I,J)-(2.*P(I,J))+POLD(I,J)))
       U(I,J) = UNEW(I,J)
       V(I,J) = VNEW(I,J)
       P(I,J) = PNEW(I,J)
       enddo
       enddo
 300    continue

       do J=1, N, 1
       UOLD(M+1,J) = UOLD(1,J)
       VOLD(M+1,J) = VOLD(1,J)
       POLD(M+1,J) = POLD(1,J)
       U(M+1,J) = U(1,J)
       V(M+1,J) = V(1,J)
       P(M+1,J) = P(1,J)
       enddo
 320    continue

       do I=1, M, 1
       UOLD(I,N+1) = UOLD(I,1)
       VOLD(I,N+1) = VOLD(I,1)
       POLD(I,N+1) = POLD(I,1)
       U(I,N+1) = U(I,1)
       V(I,N+1) = V(I,1)
       P(I,N+1) = P(I,1)
       enddo
 325    continue

       UOLD(M+1,N+1) = UOLD(1,1)
        VOLD(M+1,N+1) = VOLD(1,1)
        POLD(M+1,N+1) = POLD(1,1)
        U(M+1,N+1) = U(1,1)
        V(M+1,N+1) = V(1,1)
        P(M+1,N+1) = P(1,1)
        RETURN
        END